/**
 * Exclusive-or gate:
 * out = (((a == 0) & (b = 1)) | ((a == 1) & (b = 0)), 1, 0)
 */
 CHIP Xor {

    IN  a, b;
    OUT out;

    BUILTIN Xor;
}
