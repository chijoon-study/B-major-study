/**
 * 16-bit multiplexor: 
 * out[i] = ((sel == 0), a[i], b[i]) for i = 0..15
 */
 CHIP Mux16 {

    IN  a[16], b[16], sel;
    OUT out[16];

    BUILTIN Mux;
}
